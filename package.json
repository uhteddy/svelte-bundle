{
  "name": "svelte-bundle",
  "version": "0.0.24",
  "description": "Bundle Svelte files into static HTML files",
  "type": "module",
  "title": "svelte-bundle",
  "url": "https://github.com/uhteddy/svelte-bundle",
  "repository": "https://github.com/uhteddy/svelte-bundle",
  "description": "CLI tool to easily bundle a .svelte file into a single .html file",
  "npm": "svelte-bundle",
  "categories": [
    "build-plugins"
  ],
  "bin": {
    "svelte-bundle": "cli.js"
  },
  "files": [
    "cli.js",
    "bundle.js"
  ],
  "keywords": [
    "svelte",
    "bundle",
    "static",
    "html",
    "ssr"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uhteddy/svelte-bundle.git"
  },
  "scripts": {
    "prepublishOnly": "chmod +x cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "author": "Teddy Hartling",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "cssnano": "^7.0.6",
    "rollup": "^3.20.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.4",
    "svelte": "^3.58.0",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.3",
    "vitest": "^2.1.3"
  },
  "bugs": {
    "url": "https://github.com/uhteddy/svelte-bundle/issues"
  },
  "homepage": "https://github.com/uhteddy/svelte-bundle#readme",
  "engines": {
    "node": ">=18"
  }
}
